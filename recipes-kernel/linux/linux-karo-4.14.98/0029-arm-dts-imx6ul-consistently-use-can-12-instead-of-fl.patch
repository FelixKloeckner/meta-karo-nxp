From 40a621cd83b90376059c274f659fa184e733d325 Mon Sep 17 00:00:00 2001
From: =?UTF-8?q?Lothar=20Wa=C3=9Fmann?= <LW@KARO-electronics.de>
Date: Tue, 3 Dec 2019 10:52:36 +0100
Subject: [PATCH 2/4] arm: dts: imx6ul: consistently use 'can[12]' instead of
 'flexcan[12]' as labels

---
 arch/arm/boot/dts/imx6ul-tx6ul.dtsi                    | 8 ++++----
 arch/arm/boot/dts/imx6ul-txul-mainboard.dtsi           | 4 ++--
 arch/arm/boot/dts/imx6ul-txul-mb7.dtsi                 | 4 ++--
 arch/arm/boot/dts/imx6ul.dtsi                          | 4 ++--
 arch/arm/boot/dts/imx6ull-14x14-ddr3-arm2-flexcan2.dts | 2 +-
 arch/arm/boot/dts/imx6ull-14x14-ddr3-arm2-uart2.dts    | 2 +-
 arch/arm/boot/dts/imx6ull-14x14-ddr3-arm2.dts          | 2 +-
 arch/arm/boot/dts/imx6ull-14x14-evk.dts                | 4 ++--
 arch/arm/boot/dts/imx6ull-9x9-evk.dts                  | 4 ++--
 arch/arm/boot/dts/imx6ull.dtsi                         | 8 ++++----
 10 files changed, 21 insertions(+), 21 deletions(-)

diff --git a/arch/arm/boot/dts/imx6ul-tx6ul.dtsi b/arch/arm/boot/dts/imx6ul-tx6ul.dtsi
index b33fa42..6deb6f0 100644
--- a/arch/arm/boot/dts/imx6ul-tx6ul.dtsi
+++ b/arch/arm/boot/dts/imx6ul-tx6ul.dtsi
@@ -45,8 +45,8 @@
 
 / {
 	aliases {
-		can0 = &flexcan2;
-		can1 = &flexcan1;
+		can0 = &can2;
+		can1 = &can1;
 		display = &lcdif;
 		i2c0 = &i2c2;
 		i2c1 = &i2c_gpio;
@@ -270,14 +270,14 @@
 	};
 };
 
-&flexcan1 {
+&can1 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_flexcan1>;
 	xceiver-supply = <&reg_can_xcvr>;
 	status = "okay";
 };
 
-&flexcan2 {
+&can2 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_flexcan2>;
 	xceiver-supply = <&reg_can_xcvr>;
diff --git a/arch/arm/boot/dts/imx6ul-txul-mainboard.dtsi b/arch/arm/boot/dts/imx6ul-txul-mainboard.dtsi
index ca8045f..19f9ce2 100644
--- a/arch/arm/boot/dts/imx6ul-txul-mainboard.dtsi
+++ b/arch/arm/boot/dts/imx6ul-txul-mainboard.dtsi
@@ -49,11 +49,11 @@
 	/delete-node/ sound;
 };
 
-&flexcan1 {
+&can1 {
 	xceiver-supply = <&reg_3v3>;
 };
 
-&flexcan2 {
+&can2 {
 	xceiver-supply = <&reg_3v3>;
 };
 
diff --git a/arch/arm/boot/dts/imx6ul-txul-mb7.dtsi b/arch/arm/boot/dts/imx6ul-txul-mb7.dtsi
index 1f4575e..5836dee 100644
--- a/arch/arm/boot/dts/imx6ul-txul-mb7.dtsi
+++ b/arch/arm/boot/dts/imx6ul-txul-mb7.dtsi
@@ -49,11 +49,11 @@
 	/delete-node/ sound;
 };
 
-&flexcan1 {
+&can1 {
 	status = "disabled";
 };
 
-&flexcan2 {
+&can2 {
 	xceiver-supply = <&reg_3v3>;
 };
 
diff --git a/arch/arm/boot/dts/imx6ul.dtsi b/arch/arm/boot/dts/imx6ul.dtsi
index 0c81e47..a153546 100644
--- a/arch/arm/boot/dts/imx6ul.dtsi
+++ b/arch/arm/boot/dts/imx6ul.dtsi
@@ -505,7 +505,7 @@
 				status = "disabled";
 			};
 
-			flexcan1: flexcan@02090000 {
+			can1: flexcan@02090000 {
 				compatible = "fsl,imx6ul-flexcan", "fsl,imx6q-flexcan";
 				reg = <0x02090000 0x4000>;
 				interrupts = <GIC_SPI 110 IRQ_TYPE_LEVEL_HIGH>;
@@ -516,7 +516,7 @@
 				status = "disabled";
 			};
 
-			flexcan2: flexcan@02094000 {
+			can2: flexcan@02094000 {
 				compatible = "fsl,imx6ul-flexcan", "fsl,imx6q-flexcan";
 				reg = <0x02094000 0x4000>;
 				interrupts = <GIC_SPI 111 IRQ_TYPE_LEVEL_HIGH>;
diff --git a/arch/arm/boot/dts/imx6ull-14x14-ddr3-arm2-flexcan2.dts b/arch/arm/boot/dts/imx6ull-14x14-ddr3-arm2-flexcan2.dts
